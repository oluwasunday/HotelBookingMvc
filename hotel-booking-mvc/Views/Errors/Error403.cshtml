@{
    ViewData["Title"] = "Error";
}
<div class="container-fluid mt-5"  style="background: #F6FCFF;">
    <div class="row d-flex align-items-center justify-content-center">
        <div class="col-sm-10 col-md-10 col-lg-10 mx-auto mb-5">
            <div class="card border-0" style="background: #F6FCFF;">
                <div class="d-flex justify-content-center circle-rounded mb-0">
                    <img src="/images/404svg.svg" style="width: 502px; height: 320px;" alt="404 Image">
                </div>
                <div class="card-body text-center">
                    <h1 class="bold-font">403</h1>
                    <h3 class="bold-font-sm">Forbidden</h3>
                    <p class="card-text bold-font-p text-center w-100">You seem lost! Are you?</p>
                    <a style="text-decoration:none; color:darkblue;" class=" card-text bold-font-p text-center w-100" asp-controller="LandingView" asp-action="Index">Go Home</a>
                </div>
            </div>
        </div>
    </div>
</div>
